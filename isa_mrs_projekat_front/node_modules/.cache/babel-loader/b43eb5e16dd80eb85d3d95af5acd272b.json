{"ast":null,"code":"import { FontAwesomeIcon } from \"@fortawesome/vue-fontawesome\";\nimport { library } from \"@fortawesome/fontawesome-svg-core\";\nimport { faUser } from \"@fortawesome/free-solid-svg-icons\";\nimport { faDoorOpen } from \"@fortawesome/free-solid-svg-icons\";\nimport { faBed } from \"@fortawesome/free-solid-svg-icons\";\nimport { useStore } from \"vuex\";\nimport axios from \"axios\";\nlibrary.add(faUser);\nlibrary.add(faDoorOpen);\nlibrary.add(faBed);\nexport default {\n  name: \"CardView\",\n  components: {\n    FontAwesomeIcon\n  },\n  props: [\"rental\"],\n\n  data() {\n    return {\n      path: \"/client/RentalProfile\",\n      photo: null,\n      bedsPerRoom: null\n    };\n  },\n\n  computed: {\n    getAddress() {\n      return this.rental.address.city + ', ' + this.rental.address.country;\n    },\n\n    getPath() {\n      const store = useStore();\n      let path = \"/RentalProfile/\" + this.rental.rentalObjectType + \"-\" + this.rental.id;\n      if (store.state.email) return \"/client\" + path;\n      return \"/RentalProfile\" + path;\n    },\n\n    isVacationRental() {\n      return this.rental.rentalObjectType === \"VacationRental\";\n    },\n\n    getNumberOfRooms() {\n      return this.rental.rooms.length;\n    },\n\n    getNumberOfBeds() {\n      let beds = 0;\n      this.rental.rooms.forEach(room => {\n        beds += room.beds;\n      });\n      return beds;\n    },\n\n    getRentalGrade() {\n      if (this.rental.grade === \"0\" || !this.rental.grade) return \"/\";\n      return this.rental.grade + \"★\";\n    }\n\n  },\n\n  mounted() {\n    if (this.isVacationRental) this.numOfBedsPerRoom();\n    axios.get(\"/Photos/\", {\n      headers: {\n        Authorization: \"Bearer \" + this.$store.getters.access_token\n      },\n      params: {\n        path: this.rental.displayPhoto.photo\n      },\n      responseType: \"blob\"\n    }).then(response => {\n      this.photo = URL.createObjectURL(response.data);\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n\n  methods: {\n    numOfBedsPerRoom() {\n      let result = \"Beds per room: \";\n\n      for (let i = 0; i < this.rental.rooms.length; i++) {\n        result += this.rental.rooms[i].beds;\n        if (i === this.rental.rooms.length - 1) break;\n        result += \" + \";\n      }\n\n      this.bedsPerRoom = result;\n    },\n\n    setRentalIdAndType() {\n      this.$store.dispatch(\"rentalId\", this.rental.id);\n      this.$store.dispatch(\"rentalType\", this.rental.rentalObjectType);\n      this.$store.dispatch(\"grade\", this.rental.grade);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgEA,SAAQA,eAAR,QAA8B,8BAA9B;AACA,SAASC,OAAT,QAAwB,mCAAxB;AACA,SAASC,MAAT,QAAuB,mCAAvB;AACA,SAASC,UAAT,QAA2B,mCAA3B;AACA,SAASC,KAAT,QAAsB,mCAAtB;AACA,SAAQC,QAAR,QAAuB,MAAvB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEAL,OAAO,CAACM,GAAR,CAAYL,MAAZ;AACAD,OAAO,CAACM,GAAR,CAAYJ,UAAZ;AACAF,OAAO,CAACM,GAAR,CAAYH,KAAZ;AAEA,eAAe;AACbI,MAAI,EAAE,UADO;AAEbC,YAAU,EAAE;AACVT;AADU,GAFC;AAKbU,OAAK,EAAE,CAAC,QAAD,CALM;;AAMbC,MAAI,GAAG;AACL,WAAO;AACLC,UAAI,EAAE,uBADD;AAELC,WAAK,EAAE,IAFF;AAGLC,iBAAW,EAAE;AAHR,KAAP;AAKD,GAZY;;AAabC,UAAQ,EAAE;AACRC,cAAU,GAAG;AACX,aAAO,KAAKC,MAAL,CAAYC,OAAZ,CAAoBC,IAApB,GAA2B,IAA3B,GAAkC,KAAKF,MAAL,CAAYC,OAAZ,CAAoBE,OAA7D;AACD,KAHO;;AAIRC,WAAO,GAAG;AACR,YAAMC,KAAI,GAAIjB,QAAQ,EAAtB;AACA,UAAIO,IAAG,GAAI,oBAAoB,KAAKK,MAAL,CAAYM,gBAAhC,GAAmD,GAAnD,GAAyD,KAAKN,MAAL,CAAYO,EAAhF;AACA,UAAIF,KAAK,CAACG,KAAN,CAAYC,KAAhB,EAAuB,OAAO,YAAYd,IAAnB;AACvB,aAAO,mBAAmBA,IAA1B;AACD,KATO;;AAURe,oBAAgB,GAAG;AACjB,aAAO,KAAKV,MAAL,CAAYM,gBAAZ,KAAiC,gBAAxC;AACD,KAZO;;AAaRK,oBAAgB,GAAG;AACjB,aAAQ,KAAKX,MAAL,CAAYY,KAAb,CAAoBC,MAA3B;AACD,KAfO;;AAgBRC,mBAAe,GAAG;AAChB,UAAIC,IAAG,GAAI,CAAX;AACA,WAAKf,MAAL,CAAYY,KAAZ,CAAkBI,OAAlB,CAA0BC,IAAG,IAAK;AAChCF,YAAG,IAAKE,IAAI,CAACF,IAAb;AACD,OAFD;AAGA,aAAOA,IAAP;AACD,KAtBO;;AAuBRG,kBAAc,GAAG;AACf,UAAI,KAAKlB,MAAL,CAAYmB,KAAZ,KAAsB,GAAtB,IAA6B,CAAC,KAAKnB,MAAL,CAAYmB,KAA9C,EAAqD,OAAO,GAAP;AACrD,aAAO,KAAKnB,MAAL,CAAYmB,KAA