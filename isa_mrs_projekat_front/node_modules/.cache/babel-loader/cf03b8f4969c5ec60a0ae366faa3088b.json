{"ast":null,"code":"import axios from \"axios\";\nimport RentalAddress from \"@/components/UnregisteredUser/components/Rental/RentalAddress\";\nimport { FontAwesomeIcon } from \"@fortawesome/vue-fontawesome\";\nimport { library } from \"@fortawesome/fontawesome-svg-core\";\nimport { faPencil, faEnvelope, faPhone } from \"@fortawesome/free-solid-svg-icons\";\nlibrary.add(faPencil, faEnvelope, faPhone);\nexport default {\n  name: \"VacationRentalOwnerProfilPage\",\n  components: {\n    FontAwesomeIcon,\n    RentalAddress\n  },\n\n  data() {\n    return {\n      vacationRentalOwner: null,\n      profilePic: null\n    };\n  },\n\n  mounted() {\n    axios.get(\"/RentalOwners/getVacationRentalOwner\", {\n      headers: {\n        Authorization: \"Bearer \" + this.$store.getters.access_token\n      },\n      params: {\n        id: this.$route.params.id\n      }\n    }).then(response => {\n      this.vacationRentalOwner = response.data;\n      this.getProfilePic();\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n\n  computed: {\n    fullName() {\n      return this.vacationRentalOwner.name + \" \" + this.vacationRentalOwner.surname;\n    }\n\n  },\n  methods: {\n    getProfilePic() {\n      if (!this.vacationRentalOwner.photo) {\n        this.profilePic = null;\n        return;\n      }\n\n      axios.get(\"/Photos/\", {\n        headers: {\n          Authorization: \"Bearer \" + this.$store.getters.access_token\n        },\n        params: {\n          path: this.vacationRentalOwner.photo\n        },\n        responseType: \"blob\"\n      }).then(response => {\n        this.profilePic = URL.createObjectURL(response.data);\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4CA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,aAAP,MAA0B,+DAA1B;AACA,SAAQC,eAAR,QAA8B,8BAA9B;AACA,SAAQC,OAAR,QAAsB,mCAAtB;AACA,SAAQC,QAAR,EAAkBC,UAAlB,EAA8BC,OAA9B,QAA4C,mCAA5C;AAEAH,OAAO,CAACI,GAAR,CAAYH,QAAZ,EAAsBC,UAAtB,EAAk