{"ast":null,"code":"import { FontAwesomeIcon } from \"@fortawesome/vue-fontawesome\";\nimport { library } from \"@fortawesome/fontawesome-svg-core\";\nimport { faLocationDot } from \"@fortawesome/free-solid-svg-icons\";\nimport axios from \"axios\"; // import axios from \"axios\";\n\nlibrary.add(faLocationDot);\nexport default {\n  name: \"SubscribedClients\",\n  components: {\n    FontAwesomeIcon\n  },\n  props: [\"client\"],\n\n  data() {\n    return {\n      photo: null\n    };\n  },\n\n  mounted() {\n    if (!this.client.photo) {\n      this.setUpProfilePic();\n      return;\n    }\n\n    axios.get(\"/Photos/\", {\n      headers: {\n        Authorization: \"Bearer \" + this.$store.getters.access_token\n      },\n      params: {\n        path: this.client.photo\n      },\n      responseType: \"blob\"\n    }).then(response => {\n      this.photo = URL.createObjectURL(response.data);\n      this.setUpProfilePic();\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n\n  methods: {\n    setUpProfilePic() {\n      let picDiv = document.getElementById('imgDiv' + this.client.id);\n\n      if (!this.photo) {\n        picDiv.style.backgroundImage = \"url('\" + require('../../../../../../Images/noProfilePic.png') + \"')\";\n      } else {\n        picDiv.style.backgroundImage = \"url('\" + this.photo + \"')\";\n      }\n    }\n\n  },\n  computed: {\n    getFullName() {\n      return this.client.name + ' ' + this.client.surname;\n    },\n\n    getAddress() {\n      return this.client.address.city + ', ' + this.client.address.country;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmBA,SAAQA,eAAR,QAA8B,8BAA9B;AACA,SAASC,OAAT,QAAwB,mCAAxB;AACA,SAASC,aAAT,QAA8B,mCAA9B;AACA,OAAOC,KAAP,MAAkB,OAAlB,C,CACA;;AAEAF,OAAO,CAACG,GAAR,CAAYF,aAAZ;AAEA,eAAe;AACbG,MAAI,EAAE,mBADO;AAEbC,YAAU,EAAE;AAACN;AAAD,GAFC;AAGbO,OAAK,EAAE,CAAC,QAAD,CAHM;;AAIbC,MAAI,GAAG;AACL,WAAO;AACLC,WAAK,EAAE;AADF,KAAP;AAGD,GARY;;AASbC,SAAO,GAAG;AACR,QAAG,CAAC,KAAKC,MAAL,CAAYF,KAAhB,EAAuB;AAAE,W