{"ast":null,"code":"import axios from \"axios\";\nimport RentalAddress from \"@/components/UnregisteredUser/components/Rental/RentalAddress\";\nimport InstructorProfileReviews from \"@/components/UnregisteredUser/components/Instructor/InstructorProfileReviews\";\nimport { FontAwesomeIcon } from \"@fortawesome/vue-fontawesome\";\nimport { library } from \"@fortawesome/fontawesome-svg-core\";\nimport { faPencil } from \"@fortawesome/free-solid-svg-icons\";\nimport SubscribedClient from \"@/components/FishingInstructor/FishingInstructorPage/components/InstructorMainViews/InstructorProfile/Subscribers/SubscribedClient\";\nlibrary.add(faPencil);\nexport default {\n  name: \"VacationRentalOwnerProfilPage\",\n  components: {\n    SubscribedClient,\n    FontAwesomeIcon,\n    RentalAddress,\n    InstructorProfileReviews\n  },\n\n  data() {\n    return {\n      vacationRentalOwner: null,\n      subscribersBtnClicked: false,\n      reviewsBtnClicked: true,\n      profilePic: null\n    };\n  },\n\n  mounted() {\n    axios.get(\"/RentalOwners/getVacationRentalOwner\", {\n      headers: {\n        Authorization: \"Bearer \" + this.$store.getters.access_token\n      },\n      params: {\n        id: this.$route.params.id\n      }\n    }).then(response => {\n      this.vacationRentalOwner = response.data;\n      this.getProfilePic();\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n\n  computed: {\n    fullName() {\n      return this.vacationRentalOwner.name + \" \" + this.vacationRentalOwner.surname;\n    }\n\n  },\n  methods: {\n    getProfilePic() {\n      if (!this.vacationRentalOwner.photo) {\n        this.profilePic = null;\n        return;\n      }\n\n      axios.get(\"/Photos/\", {\n        headers: {\n          Authorization: \"Bearer \" + this.$store.getters.access_token\n        },\n        params: {\n          path: this.vacationRentalOwner.photo\n        },\n        responseType: \"blob\"\n      }).then(response => {\n        this.profilePic = URL.createObjectURL(response.data);\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkFA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,aAAP,MAA0B,+DAA1B;AACA,OAAOC,wBAAP,MAAqC,8EAArC;AACA,SAAQC,eAAR,QAA8B,8BAA9B;AACA,SAAQC,OAAR,QAAsB,mCAAtB;AACA,SAAQC,QAAR,QAAuB,mCAAvB;AACA,OAAOC,gBAAP,MACO,oIADP;AAGAF,OAAO,CAACG,GAAR,CAAYF,QAAZ;AACA,eAAe;AACbG,MAAI,EAAE,+BADO;AAEbC,YAAU,EAAE;AAACH,oBAAD;AAAmBH,mBAAnB;AAAoCF,iBAApC;AAAmDC;AAAnD,GAFC;;AAGbQ,MAAI,GAAG;AACL,WAAO;AACLC,yBAAmB,EAAE,IADhB;AAELC,2BAAqB,EAAE,KAFlB;AAGLC,uBAAiB,EAAE,IAHd;AAILC,gBAAU,EAAE;AAJP,KAAP;AAMD,GAVY;;AAWbC,SAAO,GAAG;AACRf,SAAK,CAACgB,GAAN,CAAU,sCAAV,EAAkD;AAChDC,aAAO,EAAE;AACPC,qBAAa,EAAE,YAAY,KAAKC,MAAL,CAAYC,OAAZ,CAAoBC;AADxC,OADuC;AAIhDC,YAAM,EAAE;AACNC,UAAE,EAAE,KAAKC,MAAL,CAAYF,MAAZ,CAAmBC;AADjB;AAJwC,KAAlD,EAQKE,IARL,CAQWC,QAAD,IAAc;AAClB,WAAKf,mBAAL,GAA2Be,QAAQ,CAAChB,IAApC;AACA,WAAKiB,aAAL;AACD,KAXL,EAYKC,KAZL,CAYYC,KAAD,IAAW;AAChBC,aAAO,CAACC,GAAR,CAAYF,KAAZ;AACD,KAdL;AAeD,GA3BY;;AA4BbG,UAAQ,EAAE;AACRC,YAAQ,GAAG;AACT,aAAO,KAAKtB,mBAAL,CAAyBH,IAAzB,GAAgC,GAAhC,GAAsC,KAAKG,mBAAL,CAAyBuB,OAAtE;AACD;;AAHO,GA5BG;AAiCbC,SAAO,EAAE;AACPR,iBAAa,GAAG;AACd,UAAI,CAAC,KAAKhB,mBAAL,CAAyByB,KAA9B,EAAqC;AACnC,aAAKtB,UAAL,GAAkB,IAAlB;AACA;AACF;;AACAd,WAAK,CAACgB,GAAN,CAAU,UAAV,EAAsB;AACpBC,eAAO,EAAE;AACPC,uBAAa,EAAE,YAAY,KAAKC,MAAL,CAAYC,OAAZ,CAAoBC;AADxC,SADW;AAIpBC,cAAM,EAAE;AACNe,cAAI,EAAE,KAAK1B,mBAAL,CAAyByB;AADzB,SAJY;AAOpBE,oBAAY,EAAE;AAPM,OAAtB,EASKb,IATL,CASUC,QAAO,IAAK;AAChB,aAAKZ,UAAL,GAAkByB,GAAG,CAACC,eAAJ,CAAoBd,QAAQ,CAAChB,IAA7B,CAAlB;AACD,OAXL,EAYKkB,KAZL,CAYYC,KAAD,IAAW;AAChBC,eAAO,CAACC,GAAR,CAAYF,KAAZ;AACD,OAdL;AAeF;;AArBO;AAjCI,CAAf","names":["axios","RentalAddress","InstructorProfileReviews","FontAwesomeIcon","library","faPencil","SubscribedClient","add","name","components","data","vacationRentalOwner","subscribersBtnClicked","reviewsBtnClicked","profilePic","mounted","get","headers","Authorization","$store","getters","access_token","params","id","$route","then","response","getProfilePic","catch","error","console","log","computed","fullName","surname","methods","photo","path","responseType","URL","createObjectURL"],"sourceRoot":"","sources":["C:\\Users\\Sa≈°ka\\Downloads\\MRS_ISA\\isa_mrs_projekat_front\\src\\components\\VacationRentalOwner\\VacationRentalOwnerPage\\components\\VacationRentalOwnerProfile\\VacationRentalOwnerProfilPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"page-holder ps-1\">\r\n    <div class=\"container-fluid px-lg-4 px-xl-5 p-0 m-0 pt-5 contentDiv\">\r\n      <section class=\"p-0 m-0\">\r\n        <div v-if=\"this.vacationRentalOwner\" class=\"row\">\r\n          <div class=\"col-lg-4\" style=\"height: fit-content; display:block; min-width: 18rem;\">\r\n            <div class=\"card card-profile mb-4\" >\r\n              <!--              <div class=\"card-header\"></div>-->\r\n              <div class=\"card-body text-center\">\r\n                <img v-if=\"this.profilePic\" class=\"card-profile-img\" :src=\"this.profilePic\" alt=\"\"/>\r\n                <img v-else class=\"card-profile-img\" src=\"../../../../Images/vacationRentalNoProfilePic.png\" alt=\"\"/>\r\n                <div class=\"d-flex justify-content-center\">\r\n                  <p class=\"h3\">{{ fullName }}</p>\r\n                  <router-link :to=\"'/vacationRentalOwner/profile/' + this.vacationRentalOwner.id\" class=\"btn btn-default ms-2\" style=\"height: fit-content;\">\r\n                    <font-awesome-icon class=\"h5\" icon=\"pencil\" style=\"color: #008970\"></font-awesome-icon>\r\n                  </router-link>\r\n                </div>\r\n                <hr>\r\n                <div class=\"row text-start px-3\">\r\n                  <div class=\"row\">\r\n                    <p class=\"h6\">E-mail:&emsp;{{ this.vacationRentalOwner.email }}</p>\r\n                  </div>\r\n                  <div class=\"row\">\r\n                    <p class=\"h6\">Phone:&emsp;{{ this.vacationRentalOwner.phone }}</p>\r\n                  </div>\r\n                  <div class=\"row mt-3 text-center\">\r\n                    <RentalAddress v-if=\"this.vacationRentalOwner.address\" :address=\"this.vacationRentalOwner.address\"/>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"col-lg-8\">\r\n            <div class=\"row px-3 mb-2\">\r\n              <div class=\"card tabs pt-2 pb-1\">\r\n                <ul class=\"nav justify-content-center\">\r\n                  <li class=\"nav-item\">\r\n                    <p class=\"h5\">\r\n                      <button class=\"px-3\" v-bind:style=\"{fontWeight:(reviewsBtnClicked)?700:400}\"\r\n                              @click=\"showReviews\">Reviews</button>\r\n                    </p>\r\n                  </li>\r\n                  <li class=\"nav-item\">\r\n                    <p class=\"h5\">\r\n                      <button class=\"px-3\" v-bind:style=\"{fontWeight:(subscribersBtnClicked)?700:400}\"\r\n                              @click=\"showSubscribers\">Subscribers</button>\r\n                    </p>\r\n                  </li>\r\n                </ul>\r\n              </div>\r\n            </div>\r\n            <div class=\"row px-3 pb-5\" style=\"overflow: hidden; overflow-y: scroll; max-height: 85vh;\">\r\n              <div  class=\"m-0 p-0\" v-if=\"subscribersBtnClicked\">\r\n                <div class=\"card px-0 mb-2\">\r\n                  <div class=\"card-header py-2\">\r\n                    <h4 class=\"card-heading\">Subscribers</h4>\r\n                  </div>\r\n                  <div class=\"card-body row text-align: center; justify-content: space-around;\">\r\n                    <SubscribedClient v-for=\"(subscriber, i) in vacationRentalOwner.subscribers\" :key=\"i\" :client=\"subscriber\"/>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"m-0 p-0\" v-if=\"reviewsBtnClicked\">\r\n                <div class=\"card px-0 mb-2\">\r\n                  <div class=\"card-header py-2\">\r\n                    <h4 class=\"card-heading\">Reviews</h4>\r\n                  </div>\r\n                  <div class=\"card-body\">\r\n                    <InstructorProfileReviews :reviews=\"vacationRentalOwner.clientReviews\"\r\n                                              :grade=\"vacationRentalOwner.grade\"/>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </section>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport RentalAddress from \"@/components/UnregisteredUser/components/Rental/RentalAddress\";\r\nimport InstructorProfileReviews from \"@/components/UnregisteredUser/components/Instructor/InstructorProfileReviews\";\r\nimport {FontAwesomeIcon} from \"@fortawesome/vue-fontawesome\";\r\nimport {library} from \"@fortawesome/fontawesome-svg-core\";\r\nimport {faPencil} from \"@fortawesome/free-solid-svg-icons\";\r\nimport SubscribedClient\r\n  from \"@/components/FishingInstructor/FishingInstructorPage/components/InstructorMainViews/InstructorProfile/Subscribers/SubscribedClient\";\r\n\r\nlibrary.add(faPencil);\r\nexport default {\r\n  name: \"VacationRentalOwnerProfilPage\",\r\n  components: {SubscribedClient, FontAwesomeIcon, RentalAddress, InstructorProfileReviews},\r\n  data() {\r\n    return {\r\n      vacationRentalOwner: null,\r\n      subscribersBtnClicked: false,\r\n      reviewsBtnClicked: true,\r\n      profilePic: null,\r\n    }\r\n  },\r\n  mounted() {\r\n    axios.get(\"/RentalOwners/getVacationRentalOwner\", {\r\n      headers: {\r\n        Authorization: \"Bearer \" + this.$store.getters.access_token\r\n      },\r\n      params: {\r\n        id: this.$route.params.id,\r\n      }\r\n    })\r\n        .then((response) => {\r\n          this.vacationRentalOwner = response.data;\r\n          this.getProfilePic();\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n  },\r\n  computed: {\r\n    fullName() {\r\n      return this.vacationRentalOwner.name + \" \" + this.vacationRentalOwner.surname;\r\n    },\r\n  },\r\n  methods: {\r\n    getProfilePic() {\r\n      if (!this.vacationRentalOwner.photo) {\r\n        this.profilePic = null;\r\n        return;\r\n      }\r\n      axios.get(\"/Photos/\", {\r\n        headers: {\r\n          Authorization: \"Bearer \" + this.$store.getters.access_token,\r\n        },\r\n        params: {\r\n          path: this.vacationRentalOwner.photo,\r\n        },\r\n        responseType: \"blob\"\r\n      })\r\n          .then(response => {\r\n            this.profilePic = URL.createObjectURL(response.data);\r\n          })\r\n          .catch((error) => {\r\n            console.log(error);\r\n          });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.btn-default {\r\n  cursor: pointer;\r\n  user-select: none;\r\n  color: #3f5b25;\r\n  text-decoration: none;\r\n  background-color: white;\r\n  border: none;\r\n}\r\n\r\n.btn-default:hover, .btn-default:active, .btn-default:focus {\r\n  border: 1px solid white;\r\n}\r\n\r\ndiv.tabs button {\r\n  border: none;\r\n  background-color: inherit;\r\n}\r\n\r\ndiv.tabs button:hover {\r\n\r\n}\r\n\r\n::-webkit-scrollbar {\r\n  display: none;\r\n}\r\n\r\nbutton.saveNewDataBtn {\r\n  background-color: black;\r\n  border: 1px solid black;\r\n}\r\n\r\nbutton.saveNewDataBtn:hover {\r\n  background-color: white;\r\n  color: black;\r\n}\r\n\r\n.card-header:first-child {\r\n  border-radius: calc(1rem - 1px) calc(1rem - 1px) 0 0;\r\n}\r\n\r\n.card-header {\r\n  position: relative;\r\n  padding: 2rem 2rem;\r\n  border-bottom: none;\r\n  background-color: inherit;\r\n  box-shadow: 0 0.125rem 0.25rem rgb(0 0 0 / 8%);\r\n  z-index: 2;\r\n}\r\n\r\n.card {\r\n  position: relative;\r\n  display: flex;\r\n  flex-direction: column;\r\n  height: fit-content;\r\n  min-width: 0;\r\n  word-wrap: break-word;\r\n  background-color: #fff;\r\n  background-clip: border-box;\r\n  border: none;\r\n  box-shadow: 0 0.5rem 1rem rgb(0 0 0 / 15%);\r\n  border-radius: 1rem;\r\n}\r\n\r\nh1, .h1, h2, .h2, h3, .h3, h4, .h4, h5, .h5, h6, .h6 {\r\n  line-height: 1.2;\r\n}\r\n\r\n.page-holder {\r\n  display: flex;\r\n  overflow-x: hidden;\r\n  width: 100%;\r\n  min-height: calc(100vh - 72px);\r\n  flex-wrap: wrap;\r\n}\r\n\r\na {\r\n  color: #4650dd!important;\r\n  text-decoration: underline!important;\r\n  cursor: pointer;\r\n}\r\n\r\n.card-profile-img {\r\n  margin-bottom: 1rem;\r\n  border: 3px solid #fff;\r\n  box-shadow: 0 0.5rem 1rem rgb(0 0 0 / 15%);\r\n  z-index: 2;\r\n  width: 18rem;\r\n  height: 18rem;\r\n  min-width: 12rem;\r\n  min-height: 12rem;\r\n  border-radius: 100%;\r\n  object-fit: cover;\r\n  object-position: center;\r\n}\r\n\r\nimg, svg {\r\n  vertical-align: middle;\r\n}\r\n\r\n.contentDiv {\r\n  padding-top: 4rem;\r\n}\r\n\r\n.card-profile .card-header {\r\n  height: 15rem;\r\n  background-position: center center;\r\n  background-size: cover;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}