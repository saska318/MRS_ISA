{"ast":null,"code":"import axios from \"axios\";\nimport { FontAwesomeIcon } from \"@fortawesome/vue-fontawesome\";\nimport { library } from \"@fortawesome/fontawesome-svg-core\";\nimport { faLocationDot } from \"@fortawesome/free-solid-svg-icons\";\nimport InstructorProfileAdventures from \"@/components/UnregisteredUser/components/Instructor/InstructorProfileAdventures\";\nimport InstructorProfileReviews from \"@/components/UnregisteredUser/components/Instructor/InstructorProfileReviews\";\nlibrary.add(faLocationDot);\nexport default {\n  name: \"InstructorProfile\",\n  components: {\n    InstructorProfileAdventures,\n    FontAwesomeIcon,\n    InstructorProfileReviews\n  },\n\n  data() {\n    return {\n      instructor: null,\n      adventuresBtnClicked: true,\n      reviewsBtnClicked: false,\n      profilePic: null\n    };\n  },\n\n  mounted() {\n    // this.$route.params.id\n    axios.get(\"/RentalOwners/getInstructorForClient\", {\n      headers: {\n        Authorization: \"Bearer \" + this.$store.getters.access_token\n      },\n      params: {\n        id: this.$route.params.id\n      }\n    }).then(response => {\n      this.instructor = response.data;\n      this.getProfilePic();\n    }).catch(error => {\n      console.log(error);\n    });\n  },\n\n  computed: {\n    fullName() {\n      return this.instructor.name + \" \" + this.instructor.surname;\n    },\n\n    getAddress() {\n      return this.instructor.address.city + ', ' + this.instructor.address.country;\n    }\n\n  },\n  methods: {\n    getProfilePic() {\n      if (!this.instructor.photo) {\n        this.profilePic = null;\n        return;\n      }\n\n      axios.get(\"/Photos/\", {\n        headers: {\n          Authorization: \"Bearer \" + this.$store.getters.access_token\n        },\n        params: {\n          path: this.instructor.photo\n        },\n        responseType: \"blob\"\n      }).then(response => {\n        this.profilePic = URL.createObjectURL(response.data);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    resetTabButtons() {\n      this.adventuresBtnClicked = false;\n      this.reviewsBtnClicked = false;\n    },\n\n    showAdventures() {\n      this.resetTabButtons();\n      this.adventuresBtnClicked = true;\n    },\n\n    showReviews() {\n      this.resetTabButtons();\n      this.reviewsBtnClicked = true;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAkFA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAAQC,eAAR,QAA8B,8BAA9B;AACA,SAASC,OAAT,QAAwB,mCAAxB;AACA,SAASC,aAAT,QAA8B,mCAA9B;AACA,OAAOC,2BAAP,MACO,iFADP;AAEA,OAAOC,wBAAP,MAAqC,8EAArC;AAEAH,OAAO,CAACI,GAAR,CAAYH,aAAZ;AAEA,eAAe;AACbI,MAAI,EAAE,mBADO;AAEbC,YAAU,EAAE;AAACJ,+BAAD;AAA8BH,mBAA9B;AAA+CI;AAA/C,GAFC;;AAGbI,MAAI,GAAG;AACL,WAAO;AACLC,gBAAU,EAAE,IADP;AAELC,0BAAoB,EAAE,IAFjB;AAGLC,uBAAiB,EAAE,KAHd;AAILC,gBAAU,EAAE;AAJP,KAAP;AAMD,GAVY;;AAWbC,SAAO,GAAG;AACR;AACAd,SAAK,CAACe,GAAN,CAAU,sCAAV,EAAkD;AAChDC,aAAO,EAAE;AACPC,qBAAa,EAAE,YAAY,KAAKC,MAAL,CAAYC,OAAZ,CAAoBC;AADxC,OADuC;AAIhDC,YAAM,EAAE;AACNC,UAAE,EAAE,KAAKC,MAAL,CAAYF,MAAZ,CAAmBC;AADjB;AAJwC,KAAlD,EAQKE,IARL,CAQWC,QAAD,IAAc;AAClB,WAAKf,UAAL,GAAkBe,QAAQ,CAAChB,IAA3B;AACA,WAAKiB,aAAL;AACD,KAXL,EAYKC,KAZL,CAYYC,KAAD,IAAW;AAChBC,aAAO,CAACC,GAAR,CAAYF,KAAZ;AACD,KAdL;AAeD,GA5BY;;AA6BbG,UAAQ,EAAE;AACRC,YAAQ,GAAG;AACT,aAAO,KAAKtB,UAAL,CAAgBH,IAAhB,GAAuB,GAAvB,GAA6B,KAAKG,UAAL,CAAgBuB,OAApD;AACD,KAHO;;AAIRC,cAAU,GAAG;AACX,aAAO,KAAKxB,UAAL,CAAgByB,OAAhB,CAAwBC,IAAxB,GAA+B,IAA/B,GAAsC,KAAK1B,UAAL,CAAgByB,OAAhB,CAAwBE,OAArE;AACD;;AANO,GA7BG;AAqCbC,SAAO,EAAE;AACPZ,iBAAa,GAAG;AACd,UAAG,CAAC,KAAKhB,UAAL,CAAgB6B,KAApB,EAA2B;AAAE,aAAK1B,UAAL,GAAkB,IAAlB;AAAwB;AAAQ;;AAC7Db,WAAK,CAACe,GAAN,CAAU,UAAV,EAAsB;AACpBC,eAAO,EAAE;AACPC,uBAAa,EAAE,YAAY,KAAKC,MAAL,CAAYC,OAAZ,CAAoBC;AADxC,SADW;AAIpBC,cAAM,EAAE;AACNmB,cAAI,EAAE,KAAK9B,UAAL,CAAgB6B;AADhB,SAJY;AAOpBE,oBAAY,EAAE;AAPM,OAAtB,EASKjB,IATL,CASUC,QAAO,IAAK;AAChB,aAAKZ,UAAL,GAAkB6B,GAAG,CAACC,eAAJ,CAAoBlB,QAAQ,CAAChB,IAA7B,CAAlB;AACD,OAXL,EAYKkB,KAZL,CAYYC,KAAD,IAAW;AAChBC,eAAO,CAACC,GAAR,CAAYF,KAAZ;AACD,OAdL;AAeD,KAlBM;;AAmBPgB,mBAAe,GAAG;AAChB,WAAKjC,oBAAL,GAA4B,KAA5B;AACA,WAAKC,iBAAL,GAAyB,KAAzB;AACD,KAtBM;;AAuBPiC,kBAAc,GAAG;AACf,WAAKD,eAAL;AACA,WAAKjC,oBAAL,GAA4B,IAA5B;AACD,KA1BM;;AA2BPmC,eAAW,GAAG;AACZ,WAAKF,eAAL;AACA,WAAKhC,iBAAL,GAAyB,IAAzB;AACD;;AA9BM;AArCI,CAAf","names":["axios","FontAwesomeIcon","library","faLocationDot","InstructorProfileAdventures","InstructorProfileReviews","add","name","components","data","instructor","adventuresBtnClicked","reviewsBtnClicked","profilePic","mounted","get","headers","Authorization","$store","getters","access_token","params","id","$route","then","response","getProfilePic","catch","error","console","log","computed","fullName","surname","getAddress","address","city","country","methods","photo","path","responseType","URL","createObjectURL","resetTabButtons","showAdventures","showReviews"],"sourceRoot":"","sources":["C:\\Users\\Sa≈°ka\\Downloads\\ISA_MRS\\isa_mrs_projekat_front\\src\\components\\UnregisteredUser\\components\\Instructor\\InstructorProfile.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"instructor\" class=\"page-holder ps-1\">\r\n    <div class=\"container-fluid px-lg-4 px-xl-5 p-0 m-0 pt-5 contentDiv\">\r\n      <section class=\"p-0 m-0\">\r\n        <div class=\"row\">\r\n          <div class=\"col-lg-4\" style=\"height: fit-content; display:block; min-width: 18rem;\">\r\n            <div class=\"card card-profile mb-4\" >\r\n              <!--              <div class=\"card-header\"></div>-->\r\n              <div class=\"card-body text-center\">\r\n                <img v-if=\"this.profilePic\" class=\"card-profile-img\" :src=\"this.profilePic\" alt=\"\"/>\r\n                <img v-else class=\"card-profile-img\" src=\"../../../Images/instructorNoProfilePic.png\" alt=\"\"/>\r\n                <div class=\"d-flex justify-content-center\">\r\n                  <p class=\"h3\">{{ fullName }}</p>\r\n                </div>\r\n                <hr>\r\n                <div class=\"row text-start px-3\">\r\n                  <div class=\"row\">\r\n                    <p class=\"h6\">E-mail:&emsp;{{ this.instructor.email }}</p>\r\n                  </div>\r\n                  <div class=\"row\">\r\n                    <p class=\"h6\">Phone:&emsp;{{ this.instructor.phone }}</p>\r\n                  </div>\r\n                  <div class=\"row mt-3 text-center\">\r\n                    <p class=\"h4\">\r\n                      <font-awesome-icon class=\"me-1\" icon=\"location-dot\" style=\"color: #008970\"></font-awesome-icon>\r\n                      <small>{{ getAddress }}</small>\r\n                    </p>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"col-lg-8\">\r\n            <div class=\"row px-3 mb-2\">\r\n              <div class=\"card tabs pt-2 pb-1\">\r\n                <ul class=\"nav justify-content-center\">\r\n                  <li class=\"nav-item\">\r\n                    <p class=\"h5\">\r\n                      <button class=\"px-3\" v-bind:style=\"{fontWeight:(adventuresBtnClicked)?700:400}\"\r\n                              @click=\"showAdventures\">Adventures</button>\r\n                    </p>\r\n                  </li>\r\n                  <li class=\"nav-item\">\r\n                    <p class=\"h5\">\r\n                      <button class=\"px-3\" v-bind:style=\"{fontWeight:(reviewsBtnClicked)?700:400}\"\r\n                              @click=\"showReviews\">Reviews</button>\r\n                    </p>\r\n                  </li>\r\n                </ul>\r\n              </div>\r\n            </div>\r\n            <div class=\"row px-3 pb-5\" style=\"overflow: hidden; overflow-y: scroll; max-height: 85vh;\">\r\n              <div class=\"m-0 p-0\" v-if=\"adventuresBtnClicked\">\r\n                <div class=\"card px-0 mb-2\">\r\n                  <div class=\"card-header py-2\">\r\n                    <h4 class=\"card-heading\">Adventures</h4>\r\n                  </div>\r\n                  <div class=\"card-body row text-align: center; justify-content: space-around;\">\r\n                    <InstructorProfileAdventures :adventures=\"instructor.rentalObjects\"/>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <div class=\"m-0 p-0\" v-if=\"reviewsBtnClicked\">\r\n                <div class=\"card px-0 mb-2\">\r\n                  <div class=\"card-header py-2\">\r\n                    <h4 class=\"card-heading\">Reviews</h4>\r\n                  </div>\r\n                  <div class=\"card-body\">\r\n                    <InstructorProfileReviews :reviews=\"instructor.clientReviews\"\r\n                                              :grade=\"instructor.grade\"/>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </section>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport {FontAwesomeIcon} from \"@fortawesome/vue-fontawesome\";\r\nimport { library } from \"@fortawesome/fontawesome-svg-core\";\r\nimport { faLocationDot } from \"@fortawesome/free-solid-svg-icons\";\r\nimport InstructorProfileAdventures\r\n  from \"@/components/UnregisteredUser/components/Instructor/InstructorProfileAdventures\";\r\nimport InstructorProfileReviews from \"@/components/UnregisteredUser/components/Instructor/InstructorProfileReviews\";\r\n\r\nlibrary.add(faLocationDot);\r\n\r\nexport default {\r\n  name: \"InstructorProfile\",\r\n  components: {InstructorProfileAdventures, FontAwesomeIcon, InstructorProfileReviews},\r\n  data() {\r\n    return {\r\n      instructor: null,\r\n      adventuresBtnClicked: true,\r\n      reviewsBtnClicked: false,\r\n      profilePic: null,\r\n    }\r\n  },\r\n  mounted() {\r\n    // this.$route.params.id\r\n    axios.get(\"/RentalOwners/getInstructorForClient\", {\r\n      headers: {\r\n        Authorization: \"Bearer \" + this.$store.getters.access_token\r\n      },\r\n      params: {\r\n        id: this.$route.params.id,\r\n      }\r\n    })\r\n        .then((response) => {\r\n          this.instructor = response.data;\r\n          this.getProfilePic();\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n  },\r\n  computed: {\r\n    fullName() {\r\n      return this.instructor.name + \" \" + this.instructor.surname;\r\n    },\r\n    getAddress() {\r\n      return this.instructor.address.city + ', ' + this.instructor.address.country;\r\n    },\r\n  },\r\n  methods: {\r\n    getProfilePic() {\r\n      if(!this.instructor.photo) { this.profilePic = null; return; }\r\n      axios.get(\"/Photos/\", {\r\n        headers: {\r\n          Authorization: \"Bearer \" + this.$store.getters.access_token,\r\n        },\r\n        params: {\r\n          path: this.instructor.photo,\r\n        },\r\n        responseType: \"blob\"\r\n      })\r\n          .then(response => {\r\n            this.profilePic = URL.createObjectURL(response.data);\r\n          })\r\n          .catch((error) => {\r\n            console.log(error);\r\n          });\r\n    },\r\n    resetTabButtons() {\r\n      this.adventuresBtnClicked = false;\r\n      this.reviewsBtnClicked = false;\r\n    },\r\n    showAdventures() {\r\n      this.resetTabButtons();\r\n      this.adventuresBtnClicked = true;\r\n    },\r\n    showReviews() {\r\n      this.resetTabButtons();\r\n      this.reviewsBtnClicked = true;\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.btn-default {\r\n  cursor: pointer;\r\n  user-select: none;\r\n  color: #3f5b25;\r\n  text-decoration: none;\r\n  background-color: white;\r\n  border: none;\r\n}\r\n\r\n.btn-default:hover, .btn-default:active, .btn-default:focus {\r\n  border: 1px solid white;\r\n}\r\n\r\ndiv.tabs button {\r\n  border: none;\r\n  background-color: inherit;\r\n}\r\n\r\ndiv.tabs button:hover {\r\n\r\n}\r\n\r\n::-webkit-scrollbar {\r\n  display: none;\r\n}\r\n\r\nbutton.saveNewDataBtn {\r\n  background-color: black;\r\n  border: 1px solid black;\r\n}\r\n\r\nbutton.saveNewDataBtn:hover {\r\n  background-color: white;\r\n  color: black;\r\n}\r\n\r\n.card-header:first-child {\r\n  border-radius: calc(1rem - 1px) calc(1rem - 1px) 0 0;\r\n}\r\n\r\n.card-header {\r\n  position: relative;\r\n  padding: 2rem 2rem;\r\n  border-bottom: none;\r\n  background-color: inherit;\r\n  box-shadow: 0 0.125rem 0.25rem rgb(0 0 0 / 8%);\r\n  z-index: 2;\r\n}\r\n\r\n.card {\r\n  position: relative;\r\n  display: flex;\r\n  flex-direction: column;\r\n  height: fit-content;\r\n  min-width: 0;\r\n  word-wrap: break-word;\r\n  background-color: #fff;\r\n  background-clip: border-box;\r\n  border: none;\r\n  box-shadow: 0 0.5rem 1rem rgb(0 0 0 / 15%);\r\n  border-radius: 1rem;\r\n}\r\n\r\nh1, .h1, h2, .h2, h3, .h3, h4, .h4, h5, .h5, h6, .h6 {\r\n  line-height: 1.2;\r\n}\r\n\r\n.page-holder {\r\n  display: flex;\r\n  overflow-x: hidden;\r\n  width: 100%;\r\n  min-height: calc(100vh - 72px);\r\n  flex-wrap: wrap;\r\n}\r\n\r\na {\r\n  color: #4650dd!important;\r\n  text-decoration: underline!important;\r\n  cursor: pointer;\r\n}\r\n\r\n.card-profile-img {\r\n  margin-bottom: 1rem;\r\n  border: 3px solid #fff;\r\n  box-shadow: 0 0.5rem 1rem rgb(0 0 0 / 15%);\r\n  z-index: 2;\r\n  width: 18rem;\r\n  height: 18rem;\r\n  min-width: 12rem;\r\n  min-height: 12rem;\r\n  border-radius: 100%;\r\n  object-fit: cover;\r\n  object-position: center;\r\n}\r\n\r\nimg, svg {\r\n  vertical-align: middle;\r\n}\r\n\r\n.contentDiv {\r\n  padding-top: 4rem;\r\n}\r\n\r\n.card-profile .card-header {\r\n  height: 15rem;\r\n  background-position: center center;\r\n  background-size: cover;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}