{"ast":null,"code":"import axios from \"axios\";\nimport { FontAwesomeIcon } from \"@fortawesome/vue-fontawesome\";\nimport { library } from \"@fortawesome/fontawesome-svg-core\";\nimport { faAt, faPhone } from \"@fortawesome/free-solid-svg-icons\";\nlibrary.add(faAt, faPhone);\nexport default {\n  name: \"InstructorClientPreview\",\n  props: [\"client\", \"reservationId\"],\n  components: {\n    FontAwesomeIcon\n  },\n\n  data() {\n    return {\n      profilePic: null\n    };\n  },\n\n  mounted() {\n    if (!this.client.photo) {\n      this.profilePic = \"../../../../../Images/noProfilePic.png\";\n      return;\n    }\n\n    axios.get(\"/Photos/\", {\n      headers: {\n        Authorization: \"Bearer \" + this.$store.getters.access_token\n      },\n      params: {\n        path: this.client.photo\n      },\n      responseType: \"blob\"\n    }).then(response => {\n      this.profilePic = URL.createObjectURL(response.data);\n    }).catch(error => {\n      console.log(error);\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAsDA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAAQC,eAAR,QAA8B,8BAA9B;AACA,SAAQC,OAAR,QAAsB,mCAAtB;AACA,SAAQC,IAAR,EAAcC,OAAd,QAA4B,mCAA5B;AAEAF,OAAO,CAACG,GAAR,CAAYF,IAAZ,EAAkBC,OAAlB;AAEA,eAAe;AACbE,MAAI,EAAE,yBADO;AAEbC,OAAK,EAAE,CAAC,QAAD,EAAW,eAAX,CAFM;AAGbC,YAAU,EAAE;AAACP;AAAD,GAHC;;AAIbQ,MAAI,GAAG;AACL,WAAO;AACLC,gBAAU,EAAE;AADP,KAAP;AAGD,GARY;;AASbC,SAAO,GAAG;AACR,QAAG,CAAC,KAAKC,MAAL,CAAYC,KAAhB,EAAuB;AACrB,WAAKH,UAAL,GAAkB,wCAAlB;AACA;AACF;;AACAV,SAAK,CAACc,GAAN,CAAU,UAAV,EAAsB;AACpBC,aAAO,EAAE;AACPC,qBAAa,EAAE,YAAY,KAAKC,MAAL,CAAYC,OAAZ,CAAoBC;AADxC,OADW;AAIpBC,YAAM,EAAE;AACNC,YAAI,EAAE,KAAKT,MAAL,CAAYC;AADZ,OAJY;AAOpBS,kBAAY,EAAE;AAPM,KAAtB,EASKC,IATL,CASUC,QAAO,IAAK;AAChB,WAAKd,UAAL,GAAkBe,GAAG,CAACC,eAAJ,CAAoBF,QAAQ,CAACf,IAA7B,CAAlB;AACD,KAXL,EAYKkB,KAZL,CAYYC,KAAD,IAAW;AAChBC,aAAO,CAACC,GAAR,CAAYF,KAAZ;AACD,KAdL;AAeF;;AA7Ba,CAAf","names":["axios","FontAwesomeIcon","library","faAt","faPhone","add","name","props","components","data","profileP